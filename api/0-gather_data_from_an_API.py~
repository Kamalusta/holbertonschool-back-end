#!/usr/bin/python3
"""doc"""

import urllib.request
import requests
import json
import sys


id = sys.argv[1]
res1 = requests.get(f'https://jsonplaceholder.typicode.com/users/{id}')
response1 = json.loads(res1.text)
nameusr = response1.get('name')
res = requests.get(f'https://jsonplaceholder.typicode.com/users/{id}/todos')
response = json.loads(res.text)
done = 0
ndone = 0
# print(response)
for res in response:
    if res.get('completed') == True:
        done +=1
    else:
        ndone +=1

print(f"Employee {nameusr} is done with tasks({done}/{ndone}):")
for res in response:
    if res.get('completed') is True:
        print(f"\t {res.get('title')}")
